{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": ["./types"],
    "strict": true,
    "paths": {
      "@env": ["./types/env"],
      "@/*": [
        "./*",
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
